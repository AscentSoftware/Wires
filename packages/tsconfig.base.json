{
  "compilerOptions": {
    "target": "es2015",
    "module": "commonjs",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "lib": ["es2015"],
    // do not change the lines below
    "composite": true,
    "declarationMap": true,
    "declaration": true,
    "sourceMap": true
  }
}
